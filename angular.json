{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "www",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "www": {
      "projectType": "application",
      "root": "apps/www",
      "sourceRoot": "apps/www/src",
      "prefix": "cheadle-farm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/www/browser",
            "index": "apps/www/src/index.html",
            "main": "apps/www/src/main.ts",
            "polyfills": "apps/www/src/polyfills.ts",
            "tsConfig": "apps/www/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/www/src/favicon.ico", "apps/www/src/assets"],
            "styles": ["apps/www/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/www/src/environments/environment.ts",
                  "with": "apps/www/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "www:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "www:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "www:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/www/src/**/*.ts",
              "apps/www/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/www"],
          "options": {
            "jestConfig": "apps/www/jest.config.js",
            "passWithNoTests": true
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/www/server",
            "main": "apps/www/server.ts",
            "tsConfig": "apps/www/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/www/src/environments/environment.ts",
                  "with": "apps/www/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "www:build",
            "serverTarget": "www:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "www:build:production",
              "serverTarget": "www:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "www:build:production",
            "serverTarget": "www:server:production",
            "routes": ["/"]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    },
    "www-e2e": {
      "root": "apps/www-e2e",
      "sourceRoot": "apps/www-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/www-e2e/cypress.json",
            "tsConfig": "apps/www-e2e/tsconfig.e2e.json",
            "devServerTarget": "www:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "www:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/www-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "prismic": {
      "projectType": "library",
      "root": "libs/prismic",
      "sourceRoot": "libs/prismic/src",
      "prefix": "cheadle-farm",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/prismic/src/**/*.ts",
              "libs/prismic/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/prismic"],
          "options": {
            "jestConfig": "libs/prismic/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "menu": {
      "projectType": "library",
      "root": "libs/menu",
      "sourceRoot": "libs/menu/src",
      "prefix": "cheadle-farm",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/menu/src/**/*.ts",
              "libs/menu/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/menu"],
          "options": {
            "jestConfig": "libs/menu/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "google-maps": {
      "projectType": "library",
      "root": "libs/google-maps",
      "sourceRoot": "libs/google-maps/src",
      "prefix": "cheadle-farm",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/google-maps/src/**/*.ts",
              "libs/google-maps/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/google-maps"],
          "options": {
            "jestConfig": "libs/google-maps/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
